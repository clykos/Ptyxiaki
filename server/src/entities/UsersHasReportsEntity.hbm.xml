<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.UsersHasReportsEntity" table="users_has_reports" schema="" catalog="project">
        <composite-id mapped="true" class="entities.UsersHasReportsEntityPK">
            <key-property name="usersIdusers" column="users_idusers"/>
            <key-property name="reportsIdreports" column="reports_idreports"/>
        </composite-id>
    </class>
</hibernate-mapping>